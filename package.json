{
  "types": "module",
  "name": "akyos-animation",
  "version": "1.1.5",
  "description": "Akyos Animation est une librairie JavaScript légère et modulaire permettant d'ajouter facilement des animations fluides et performantes via des classes ou des attributs HTML",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akyoscommunication/akyos-animation.git"
  },
  "main": "dist/akyos-animations.umd.js",
  "module": "dist/akyos-animations.es.js",
  "exports": {
    ".": {
      "require": "./dist/akyos-animations.umd.js",
      "import": "./dist/akyos-animations.es.js"
    }
  },
  "scripts": {
    "docs": "jsdoc -c jsdoc.json",
    "test": "jest --no-cache",
    "build": "vite build && npm run test",
    "prepare": "npm run build"
  },
  "keywords": [
    "animation",
    "gsap",
    "akyos",
    "akyos-animation",
    "akyoscommunication",
    "javascript",
    "scroll animations",
    "modular animations"
  ],
  "peerDependencies": {
    "gsap": "^3.12.7",
    "lenis": "^1.1.21"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc": "^3.6.11",
    "node-sass": "^8.0.0",
    "vite": "^6.2.0"
  },
  "type": "module",
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "gsap": "^3.12.7",
    "jest-mock": "^29.7.0"
  }
}
